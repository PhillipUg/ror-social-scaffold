<section class="users-section">
  <ul class="users-list">
    <% @users.each do |user| %>
      <li>
        Name: <%= user.name %>
        <span class="profile-link">
          <%= link_to 'See Profile',  user_path(user), class: 'profile-link' %>
          <% if current_user != user %>
            <%= link_to 'Invite to friendship',  invite_path(friend_id: user.id) %>
          <% end %>
        </span>
      </li>
    <% end %>
  </ul>
</section>
